        </JFXButton>
        <JFXButton id="submit-button" fx:id="ninth" text=":|">
            <HBox.margin>
                <Insets left="23.0"/>
            </HBox.margin>
        </JFXButton>
        <JFXButton id="submit-button" fx:id="tenth" text="|:">
            <HBox.margin>
                <Insets left="23.0"/>
            </HBox.margin>
        </JFXButton>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="3">
        <JFXButton id="submit-button" fx:id="add" disable="true" text="+">
            <HBox.margin>
                <Insets left="403.0"/>
            </HBox.margin>
        </JFXButton>
        <JFXButton id="submit-button" fx:id="minus" disable="true" text="-">
            <HBox.margin>
                <Insets left="84.0"/>
