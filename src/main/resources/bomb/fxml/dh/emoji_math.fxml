        </JFXButton>
        <JFXButton id="submit-button" fx:id="equal" disable="true" onAction="#submitEquation" text="=">
            <HBox.margin>
                <Insets left="84.0"/>
            </HBox.margin>
        </JFXButton>
        <JFXButton id="submit-button" fx:id="clear" disable="true" onAction="#resetModule" text="Clear">
            <HBox.margin>
                <Insets left="280.0" top="60.0"/>
            </HBox.margin>
        </JFXButton>
    </HBox>
