    </rowConstraints>
    <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
        <JFXTextField fx:id="equationBar" editable="false" promptText="Equation">
            <VBox.margin>
                <Insets left="150.0" right="150.0"/>
