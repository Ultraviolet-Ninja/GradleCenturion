        </JFXButton>
        <JFXButton id="submit-button" fx:id="third" text="(:">
            <HBox.margin>
                <Insets left="23.0"/>
            </HBox.margin>
        </JFXButton>
        <JFXButton id="submit-button" fx:id="forth" text=")=">
            <HBox.margin>
                <Insets left="23.0"/>
            </HBox.margin>
        </JFXButton>
        <JFXButton id="submit-button" fx:id="fifth" text=":(">
            <HBox.margin>
                <Insets left="23.0"/>
            </HBox.margin>
        </JFXButton>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
        <JFXButton id="submit-button" fx:id="sixth" text="):"/>
